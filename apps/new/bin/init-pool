#!/bin/sh

# init-pool
# POOL_DIR - path to pool
# TEST_DIR - path to test dir
# CC - compiler
# FLAGS - compiler flags

mkdir $POOL_DIR/run 2> /dev/null

# directory with private configs
CONFIG_DIR="$TEST_DIR/private-etc"


#echo $CC $FLAGS $TEST_DIR/src/new/new.c -lm -o $POOL_DIR/run/new
#$CC $FLAGS $TEST_DIR/src/new/new.c -lm -o $POOL_DIR/run/new
$TEST_DIR/src/new.sh $FLAGS > $POOL_DIR/run/new
